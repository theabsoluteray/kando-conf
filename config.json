{
  "version": "2.1.0",
  "locale": "auto",
  "showIntroductionDialog": true,
  "menuTheme": "neon-lights",
  "darkMenuTheme": "neon-lights",
  "menuThemeColors": {
    "neon-lights": {
      "glow-color": "rgb(0 0 0)",
